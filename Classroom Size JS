function createClassroom()
{
rn = window.prompt("Input length of the classroom in feet.", 1);
cn = window.prompt("Input width of the classroom in feet.",1);
  
 for(var r=0;r<parseInt(rn,10);r++)
  {
   var x=document.getElementById('myClassroom').insertRow(r);
   for(var c=0;c<parseInt(cn,10);c++)  
    {
     var y=  x.insertCell(c);
     y.innerHTML=""+r+","+c; 
    }
   }
}
